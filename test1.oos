        self.value = v;
        ;
    
    def get_value(self): int
        return self.value;
        ;
    
    def is_positive(self): int
        int result;
        ;
        result = 0;
        if (self.value > 0):
            result = 1;
        else:
            result = 0;
        endif;
        return result;
        ;
    
    def sum_to_value(self): int
        int sum, i;
        ;
        sum = 0;
        i = 1;
        while (i <= self.value):
            sum = sum + i;
            i = i + 1;
        endwhile;
        return sum;
